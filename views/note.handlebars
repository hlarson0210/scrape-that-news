<ul class="sidenav" id="mobile-demo">
    <li><a href="/">Home</a></li>
    <li><a href="/saved-articles">Saved Articles</a></li>
    <li><button class="waves-effect waves-light btn beer-btn beer-me">Beer me Articles</button></li>
</ul>


<div class="parallax-container">
    <div class="parallax"><img
            src="https://www.wallpaperup.com/uploads/wallpapers/2012/07/21/6690/46c0209307e2a2b7daa6fa14436c4d7e-700.jpg">
    </div>
    <h1 class="parallaxText center">Saved Articles</h1>
</div>

<div class="container">
    <div class="container">
        <div class="row">
            {{#each Article}}
            <ul class="collection">
                <li class="collection-item avatar" data-_id={{_id}}>
                    <i class="material-icons circle">question_answer</i>
                    <span class="title"><a target="_blank" href={{link}}>
                            <h5>{{title}}</h5>
                        </a></span>
                    <p>
                        <h6>{{#if summary}}{{summary}}{{else}}No summary for this article{{/if}}</h6>
                    </p>
                    <a href="#!" class="secondary-content"><i class="material-icons" id="favNow">favorite_border</i></a>
                </li>
            </ul>
            {{/each}}
        </div>
    </div>
</div>