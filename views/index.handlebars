<nav style="background-color: #050d35;">
    <div class="container">
        <div class="row">
            <div class="nav-wrapper">
                <h6 class="brand-logo offset-s3">Mongo Scraper</h6>
                <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
                <ul class="right hide-on-med-and-down">
                    <li><a href="/">Home</a></li>
                    <li><a href="/saved-articles">Saved Articles</a></li>
                    <li><a id="clear-articles">Clear Articles</a></li>
                    <li><button class="waves-effect waves-light btn beer-btn beer-me">Beer me Articles</button>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</nav>

<ul class="sidenav" id="mobile-demo">
    <li><a href="/">Home</a></li>
    <li><a href="/saved-articles">Saved Articles</a></li>
    <li><a id="clear-articles">Clear Articles</a></li>
    <li><button class="waves-effect waves-light btn beer-btn beer-me">Beer me Articles</button></li>
</ul>


<div class="parallax-container">
    <div class="parallax"><img src="https://media.nbcboston.com/images/652*367/julyGettyImages-467241850.jpg">
    </div>
    <h1 class="parallaxText center white-text">Scrape the News</h1>
</div>

<div class="container">
    <div class="row">
        {{#each Article}}
        <ul class="collection" data-_id={{_id}}>
            <li class="collection-item avatar">
                <i class="material-icons circle">question_answer</i>
                <span class="title"><a target="_blank" href={{link}}>
                        <h5>{{title}}</h5>
                    </a></span>
                <p>
                    <h6>{{#if summary}}{{summary}}{{else}}No summary for this article{{/if}}</h6>
                </p>
                <span class="favNow">
                <a href="#!" class="secondary-content"><i class="material-icons">favorite_border</i></a></span>
            </li>
        </ul>
        {{/each}}
    </div>
</div>